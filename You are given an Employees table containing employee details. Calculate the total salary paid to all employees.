CREATE TABLE Employees (
    EmpID INT PRIMARY KEY,
    Name VARCHAR(50),
    Department VARCHAR(50),
    Salary DECIMAL(10,2)
);

INSERT INTO Employees VALUES
(1, 'Yash', 'IT', 60000.00),
(2, 'Ravi', 'HR', 50000.00),
(3, 'Aman', 'Finance', 55000.00),
(4, 'Neha', 'IT', 70000.00),
(5, 'Priya', 'Marketing', 48000.00);

select sum(Salary) Total_Salary from Employees;
